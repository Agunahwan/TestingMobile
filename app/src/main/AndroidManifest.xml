<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.agunahwan.kuis">

    <!-- Internet permission -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.agunahwan.kuis.MainActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="com.agunahwan.kuis.ScoreFinalActivity"
            android:label="@string/title_activity_score_final"></activity>
        <activity
            android:name="com.agunahwan.kuis.LoginActivity"
            android:label="@string/title_activity_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.agunahwan.kuis.TipeSoalActivity"
            android:label="@string/title_activity_tipe_soal"></activity>
    </application>

</manifest>
